{
  "name": "auto-service",
  "version": "0.0.0",
  "description": "auto service client",
  "scripts": {
    "prepare": "cd .. && husky install client/configs/husky",
    "build:development": "cd tools && node prepare-build-folder && webpack --config ../configs/webpack.js",
    "build:production": "cd tools && node prepare-build-folder && webpack --config ../configs/webpack.js",
    "development:server": "cd tools && node prepare-build-folder && cd server && node -r dotenv/config . dotenv_config_path=../../configs/environment/.local",
    "development:storybook": "start-storybook -c configs/storybook -p 6006",
    "task:lint": "eslint . --ext .js,.jsx --ignore-pattern node_modules --config ./configs/eslint.js --fix"
  },
  "author": "Polyakov Alexey",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-interactions": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-library": "0.0.11",
    "@svgr/webpack": "^6.1.2",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-react-docgen": "^4.2.1",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-webpack-plugin": "^3.1.1",
    "express": "^4.17.1",
    "husky": "^8.0.1",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "react-docgen": "^5.4.1",
    "style-loader": "^3.3.1",
    "stylus": "^0.56.0",
    "stylus-loader": "^6.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-middleware": "^5.2.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "core-js": "^3.19.2",
    "date-fns": "^2.28.0",
    "final-form": "^4.20.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-input-mask": "^2.0.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.1.1",
    "redux": "^4.2.0"
  }
}
